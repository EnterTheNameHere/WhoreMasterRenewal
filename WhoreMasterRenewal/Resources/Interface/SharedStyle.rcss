body
{
    font-family: Lacuna;
    font-weight: normal;
    font-style: normal;
    font-size: 15;
    color: white;
    
    /*padding-top: 43px;*/
    /*padding-bottom: 20px;*/
    
    min-width: 800px;
    min-height: 600px;
    
    width: 100%;
    height: 100%;
    
    /*margin: auto;*/
}
